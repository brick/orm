{
  "name": "brick/orm",
  "description": "Object-Relational Mapper",
  "type": "library",
  "keywords": [
    "ORM",
    "Data Mapper"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "brick/db": "dev-interfaces"
  },
  "require-dev": {
    "ext-pdo": "*",
    "phpunit/phpunit": "^10.1",
    "php-coveralls/php-coveralls": "^2.0",
    "vimeo/psalm": "5.24.0"
  },
  "autoload": {
    "psr-4": {
      "Brick\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\ORM\\Tests\\": "tests/"
    }
  },
  "bin": ["bin/orm-build"]
}
