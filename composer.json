{
  "name": "brick/orm",
  "description": "Object-Relational Mapper",
  "type": "library",
  "keywords": [
    "ORM",
    "Data Mapper"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "brick/db": "dev-interfaces",
    "brick/reflection": "0.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
